	padding-bottom: 5px;
	padding-top: 5px;
}
nav
{
	text-align: right;
}
nav a
{
	color: #444444;
}
#mainContainer
{
	background: url("/static/images/36kr/nav_bg.png") repeat scroll 0 0 transparent;
}
#mainContainer.container-fluid
{
	box-sizing: border-box;
	left: 0;
	moz-box-sizing: border-box;
	ms-box-sizing: border-box;
	o-box-sizing: border-box;
	padding: 0 20px 20px;
	right: 0;
	webkit-box-sizing: border-box;
	width: 100%;
}
.bcrumbs
{
	list-style: none;
}
.bcrumbs li
{
	color: rgba(0,0,0,0.3);
	cursor: pointer;
	display: block;
	float: left;
	font-size: 22px;
	margin-left: 8px;
	margin-right: 8px;
	moz-transition: color 0.1s ease-in-out;
	ms-transition: color 0.1s ease-in-out;
	o-transition: color 0.1s ease-in-out;
	transition: color 0.1s ease-in-out;
	webkit-transition: color 0.1s ease-in-out;
}
.bcrumbs li:hover:not(.divdot):not(last-child)
{
	color: rgba(0,0,0,0.4);
}
.bcrumbs li:last-child
{
	color: #00afdb;
	margin-right: 0px;
}
.sideBar
{
	overflow-x: hidden;
	overflow-y: hidden;
	padding-top: 20px;
}
.sideBar a
{
	color: #141F40;
}
.sideBar ul
{
	list-style: none;
	margin: 0;
}
.sideBar ul>li
{
	color: #141F40;
	cursor: pointer;
	font-size: 16px;
	margin-bottom: 2px;
}
.sideBar ul>li figure
{
	margin: 0;
	padding: 6px 14px 6px 14px;
}
.sideBar ul>li.active figure:first-child
{
	color: #00afdb;
}
.sideBar ul>li i
{
	display: inline-block;
}
.sideBar ul>li .menuText
{
	display: inline-block;
}
.sideBar ul.subSide
{
	display: none;
	padding: 5px 0px 5px 0px;
}
.sideBar ul.subSide li
{
	font-size: 15px;
	line-height: 17px;
	margin-bottom: 5px;
	margin-left: 10px;
	padding: 4px 10px 5px 14px;
}
.sideBar ul.subSide li a:hover
{
	color: #286BA7;
}
.content
{
	background: url("/static/images/36kr/content_bg.png") repeat scroll 0 0 transparent;
	box-shadow: 0 0 5px 0 silver;
	padding: 10px 20px;
}
.item
{
	padding-bottom: 10px;
}
.item .cTitle
{
	color: #00afdb;
	font-size: 22px;
	margin-bottom: 10px;
}
.item p
{
	font-size: 14px;
	line-height: 1.8;
	padding: 5px;
}
.item p:first-child
{
	margin-bottom: 10px;
}
hr
{
	border: 0;
	margin: -6px 0;
}
.borBox
{
	box-sizing: border-box;
	moz-box-sizing: border-box;
	ms-box-sizing: border-box;
	o-box-sizing: border-box;
	webkit-box-sizing: border-box;
}
.btn
{
	border-radius: 1px;
	moz-border-radius: 1px;
	webkit-border-radius: 1px;
}
.btn-group.sharp .btn:first-child
{
	border-radius: 1px 0px 0px 1px;
	moz-border-radius: 1px 0px 0px 1px;
	webkit-border-radius: 1px 0px 0px 1px;
}
.btn-group.sharp .btn:last-child
{
	border-radius: 0px 1px 1px 0px;
	moz-border-radius: 0px 1px 1px 0px;
	webkit-border-radius: 0px 1px 1px 0px;
}
.btn-group.active-group .btn
{
	background-image: none;
}
.btn-group.rating .btn
{
	background-color: #CCC;
	background-image: none;
	box-shadow: none;
}
.btn.noGR
{
	background-image: none;
}
.modal h3 i
{
	margin-top: 4px !important;
}
.modal form i
{
	margin-top: 2px !important;
}
#uOpsL
{
	margin-top: -10px;
}
.page-header
{
	margin-top: 0;
	padding-top: 0;
}
.hBig
{
	color: #2076bc;
	font-size: 22px;
	font-weight: lighter;
}
.hBig small
{
	color: rgba(0,0,0,0.4);
	font-size: 18px;
}
.hBig p
{
	margin-top: 10px;
}
.box
{
	moz-transition: background 0.2s ease-in-out;
	ms-transition: background 0.2s ease-in-out;
	o-transition: background 0.2s ease-in-out;
	transition: background 0.2s ease-in-out;
	webkit-transition: background 0.2s ease-in-out;
}
.notification
{
	background-color: rgba(0,175,219,0.6);
	border: 2px #fff solid;
	border-radius: 2px;
	box-shadow: 0px 1px 4px 0px rgba(0,0,0,0.06);
	color: #FFF;
	font-size: 15px;
	left: 50%;
	padding: 5px 14px 6px 14px;
	position: absolute;
	text-align: center;
	text-shadow: 1px 1px 1px rgba(0,0,0,0.1);
	top: 9px;
	width: auto;
}
.stripe
{
	background-image: linear-gradient(-45deg, rgba(255,255,255,0.25) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.25) 50%, rgba(255,255,255,0.25) 75%, transparent 75%, transparent);
	background-image: -moz-linear-gradient(-45deg, rgba(255,255,255,0.25) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.25) 50%, rgba(255,255,255,0.25) 75%, transparent 75%, transparent);
	background-image: -ms-linear-gradient(-45deg, rgba(255,255,255,0.25) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.25) 50%, rgba(255,255,255,0.25) 75%, transparent 75%, transparent);
	background-image: -o-linear-gradient(-45deg, rgba(255,255,255,0.25) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.25) 50%, rgba(255,255,255,0.25) 75%, transparent 75%, transparent);
	background-image: -webkit-gradient(linear, 0 100%, 100% 0, color-stop(0.25, rgba(255,255,255,0.25)), color-stop(0.25, transparent), color-stop(0.5, transparent), color-stop(0.5, rgba(255,255,255,0.25)), color-stop(0.75, rgba(255,255,255,0.25)), color-stop(0.75, transparent), to(transparent));
	background-image: -webkit-linear-gradient(-45deg, rgba(255,255,255,0.25) 25%, transparent 25%, transparent 50%, rgba(255,255,255,0.25) 50%, rgba(255,255,255,0.25) 75%, transparent 75%, transparent);
}
.stripeB
{
	background-size: 60px 60px;
	moz-background-size: 60px 60px;
	ms-background-size: 60px 60px;
	o-background-size: 60px 60px;
	webkit-background-size: 60px 60px;
}
.stripeS
{
	background-size: 40px 40px;
	moz-background-size: 40px 40px;
	ms-background-size: 60px 60px;
	o-background-size: 40px 40px;
	webkit-background-size: 40px 40px;
}
.stripe.activeB
{
	animation: stripeB 0.8s linear infinite;
	moz-animation: stripeB 0.8s linear infinite;
	ms-animation: stripeB 0.8s linear infinite;
	o-animation: stripeB 0.8s linear infinite;
	webkit-animation: stripeB 0.8s linear infinite;
}